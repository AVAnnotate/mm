    {
      "start_time": 6,
      "end_time": 11,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Ladies and gentleman, it's Halloween and here's a man I think you're all going to want to meet. Say hello, please, to Mr. Bobby Boris Pickett. Hi Bobby."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Clint Holmes",
          "category": "Speaker"
        }
      ],
      "uuid": "4c525bd5-9279-46cf-9721-09c543bbdfc4"
    },
    {
      "start_time": 12,
      "end_time": 15,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Hi! Happy Halloween."
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Bobby Pickett",
          "category": "Speaker"
        }
      ],
      "uuid": "47248ede-936e-46e3-8eac-d41850bd11d6"
    },
    {
      "start_time": 15,
      "end_time": 15,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Thank you!"
            }
          ]
        }
      ],
      "tags": [
        {
          "tag": "Clint Holmes",
          "category": "Speaker"
        }
      ],
      "uuid": "ea439907-f9e3-43cf-a6da-7e8f0ed111ef"
    },
    {
      "start_time": 16,
      "end_time": 17,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Nice to be here."
            }
          ]
        }
